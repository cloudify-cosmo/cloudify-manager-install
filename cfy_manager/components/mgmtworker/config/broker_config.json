{
  "broker_ssl_enabled": true,
  "broker_cert_path": "{{ rabbitmq.broker_cert_path }}",
  "broker_username": "{{ rabbitmq.username }}",
  "broker_password": "{{ rabbitmq.password }}",
  "broker_hostname": "{{ rabbitmq.cluster_members.values.0.networks.default }}",
  "broker_management_hostname": "{% if rabbitmq.management_only_local %}127.0.0.1{% else %}{{ rabbitmq.cluster_members.values.0.networks.default }}{% endif %}",
  "broker_vhost": "/"
}
